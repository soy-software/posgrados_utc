{
    "name": "tavo1987/laravel-ec-validator",
    "description": "Package to validate ruc and ci Ecuador",
    "keywords": ["cedula", "laravel", "ecuador"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tavo1987",
            "email": "tavo198718@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "tavo1987/ec-validador-cedula-ruc": "^1.0.2",
        "illuminate/validation": "^7.4.0",
        "illuminate/support": "^7.4.0"
    },

    "autoload": {
        "psr-4": {
            "Tavo\\EcLaravelValidator\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tavo\\EcLaravelValidator\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.1.0",
        "phpunit/phpunit": "^9.1.1"
    },

    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },

    "extra": {
        "laravel": {
            "providers": [
                "Tavo\\EcLaravelValidator\\EcValidatorServiceProvider"
            ]
        }
    }

}
